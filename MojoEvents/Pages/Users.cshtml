@page
@model MojoEvents.Pages.UsersModel
@{
    ViewData["Title"] = "Users";
    ViewData["CurrentPage"] = "users";
}

<h2>Sub-Users</h2>
<hr />
<p>Deze lijst bevat al uw sub-users, deze users kunnen vooraf bepaalde dingen aan uw festival veranderen.</p>

<table>
    <tr>
        <th colspan="2">
            Gebruikers
        </th>
    </tr>
    @if (Model.Users != null)
    {
        @while (Model.Users.Read())
        {
            <tr>
                <td>
                    @Model.Users.GetValue(1)
                </td>
                <td>
                    <img src="~/Images/DropDownIcon.png" id="Target" />
                </td>
            </tr>
            <tr>
                <td class="DropMenu">
                    Verander festival
                </td>
            </tr>
        }
    }
</table>